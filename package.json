{
  "name": "termenv",
  "version": "0.1.1",
  "description": "Terminal Environment Utilities",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@9.4.0",
  "license": "MIT",
  "homepage": "https://github.com/luxass/termenv",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/termenv.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/termenv/issues"
  },
  "keywords": [
    "color",
    "colour",
    "colors",
    "terminal",
    "console",
    "ansi",
    "tty",
    "support",
    "supports",
    "farver"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "browser": "./dist/browser/index.mjs",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "browser": "./dist/browser/index.cjs",
        "default": "./dist/index.cjs"
      },
      "browser": "./dist/browser/index.mjs"
    },
    "./browser": {
      "import": {
        "types": "./dist/browser/index.d.ts",
        "browser": "./dist/browser/index.mjs",
        "default": "./dist/browser/index.mjs"
      },
      "require": {
        "types": "./dist/browser/index.d.cts",
        "browser": "./dist/browser/index.cjs",
        "default": "./dist/browser/index.cjs"
      },
      "browser": "./dist/browser/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --report-unused-disable-directives",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.8.2",
    "@types/node": "^18.19.39",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "jsdom": "^24.1.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  }
}
